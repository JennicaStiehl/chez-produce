<h1>Browse All Items</h1>
<% @users.each do |user| %>
  <%= user.name %>
  <%= user.items.each do |item| %>
    <%= link_to "#{item.name}", item_path(item) %>
    <%= item.current_price %>
    <%= item.inventory %>
    <img src="<%=link_to "#{item.image}", item_path(item) %>" alt="#{item.id}">
  <% end %>
<% end %>
